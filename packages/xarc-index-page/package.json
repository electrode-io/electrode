{
  "name": "@xarc/index-page",
  "version": "1.2.3",
  "description": "Electrode X template and handlers for generating index.html webpage",
  "main": "dist/index.js",
  "scripts": {
    "build": "xrun user/build",
    "test": "xrun xarc/test-only",
    "lint": "xrun xarc/lint",
    "coverage": "xrun xarc/test-cov",
    "check": "xrun xarc/check",
    "prepublishOnly": "xrun --serial [[build, docs], xarc/check]",
    "docs": "xrun xarc/docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electrode-io/electrode.git"
  },
  "keywords": [
    "webapp",
    "index-page"
  ],
  "dependencies": {
    "@xarc/jsx-renderer": "^1.0.15",
    "@xarc/render-context": "^1.0.12",
    "@xarc/tag-renderer": "^1.0.14",
    "lodash": "^4.17.21",
    "require-at": "^1.0.6"
  },
  "author": "Walmart GTP.js",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/electrode-io/electrode/issues"
  },
  "homepage": "https://github.com/electrode-io/electrode#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.12",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "@xarc/module-dev": "^5.0.0",
    "@xarc/run": "^1.1.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^8.52.0",
    "eslint-config-walmart": "^2.2.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jsdoc": "^48.2.6",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "sinon": "^18.0.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "xsh": "^0.4.5",
    "xstdout": "^0.1.1"
  },
  "files": [
    "dist",
    "src"
  ],
  "nyc": {
    "extends": [
      "@istanbuljs/nyc-config-typescript"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "xrun-tasks.*",
      "coverage",
      "dist",
      "docs",
      "gulpfile.js",
      "test"
    ],
    "check-coverage": true,
    "statements": 29.59,
    "branches": 8.33,
    "functions": 12.1,
    "lines": 30,
    "cache": false
  },
  "mocha": {
    "require": [
      "./config/test/setup.js",
      "ts-node/register",
      "source-map-support/register",
      "@xarc/module-dev/config/test/setup.js"
    ],
    "recursive": true
  },
  "@xarc/module-dev": {
    "features": [
      "eslint",
      "mocha",
      "typedoc",
      "typescript"
    ]
  }
}
