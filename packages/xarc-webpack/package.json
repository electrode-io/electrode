{
  "name": "@xarc/webpack",
  "version": "11.1.0",
  "description": "Electrode X webpack config",
  "main": "lib/index.js",
  "homepage": "http://www.electrode.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/electrode.git"
  },
  "bin": {
    "xarc-webpack-cli": "./bin/xarc-webpack-cli.js"
  },
  "bugs": {
    "url": "https://github.com/electrode-io/electrode/issues"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "xrun user/build",
    "test": "xrun xarc/test-only",
    "coverage": "xrun xarc/test-cov",
    "check": "xrun xarc/check",
    "lint": "clap lint",
    "prepublishOnly": "xrun [[build, docs], xarc/check]",
    "docs": "xrun xarc/docs"
  },
  "files": [
    "bin",
    "lib",
    "src"
  ],
  "author": "Electrode (http://www.electrode.io/)",
  "contributors": [
    "Joel Chen <xchen@walmartlabs.com>"
  ],
  "dependencies": {
    "@xarc/dev-base": "^0.1.0",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "chalker": "^1.2.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "electrode-cdn-file-loader": "^1.0.0",
    "electrode-node-resolver": "^2.0.2",
    "file-loader": "^6.2.0",
    "filter-scan-dir": "1.1.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.3.9",
    "mkdirp": "^1.0.3",
    "optional-require": "^1.1.6",
    "require-at": "^1.0.6",
    "url-loader": "^4.1.0",
    "webpack": "^5.33.2",
    "webpack-cli": "4.8.0",
    "webpack-config-composer": "^1.1.5",
    "webpack-stats-plugin": "^1.0.3",
    "xsh": "^0.4.5"
  },
  "peerDependencies": {
    "@xarc/app": "^10.0.12",
    "@xarc/app-dev": "^10.1.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.7.6",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@xarc/app": "^10.0.12",
    "@xarc/module-dev": "^2.2.5",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-walmart": "^2.2.1",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "prettier": "^1.14.2",
    "run-verify": "^1.2.1",
    "sinon": "^7.2.6",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">= 12",
    "npm": ">= 6"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "*clap.js",
      "*clap.ts",
      "coverage",
      "dist",
      "docs",
      "gulpfile.js",
      "test",
      "xrun*.js",
      "xrun*.ts",
      "lib"
    ],
    "check-coverage": false,
    "statements": 0,
    "branches": 0,
    "functions": 0,
    "lines": 0,
    "cache": true,
    "extends": [
      "@istanbuljs/nyc-config-typescript"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/",
    "access": "public"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "@xarc/module-dev/config/test/setup.js"
    ],
    "recursive": true
  },
  "@xarc/module-dev": {
    "features": [
      "eslint",
      "eslintTS",
      "mocha",
      "typedoc",
      "typescript"
    ]
  }
}
