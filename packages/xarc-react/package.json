{
  "name": "@xarc/react",
  "version": "1.0.6",
  "description": "xarc web app platform support for React framework",
  "main": "dist-node-cjs/node/index.js",
  "module": "dist-node-esm/node/index.js",
  "browser": {
    "./dist-node-cjs/node/index.js": "./dist-browser~es5~cjs~/browser/index.js",
    "./dist-node-esm/node/index.js": "./dist-browser~es2x~esm~/browser/index.js"
  },
  "scripts": {
    "build": "xrun user/build",
    "test": "xrun xarc/test-only",
    "coverage": "xrun xarc/test-cov",
    "check": "xrun xarc/check",
    "prepublishOnly": "xrun [[build, docs], xarc/check]",
    "docs": "xrun xarc/docs"
  },
  "keywords": [],
  "author": "Electrode",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/react": ">=18",
    "@xarc/render-context": "^1.0.13",
    "@xarc/subapp": "^0.4.2",
    "@xarc/tag-renderer": "^1.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/runtime": "^7.22.3",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.14.10",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.4",
    "@types/sinon": "^10.0.11",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@xarc/module-dev": "^4.1.0",
    "@xarc/run": "^1.1.1",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-config-walmart": "^2.2.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jsdoc": "^48.2.7",
    "eslint-plugin-tsdoc": "^0.3.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "xsh": "^0.4.5"
  },
  "files": [
    "dist",
    "dist-browser~es2x~esm~",
    "dist-browser~es5~cjs~",
    "dist-node-cjs",
    "dist-node-esm",
    "src"
  ],
  "nyc": {
    "extends": [
      "@istanbuljs/nyc-config-typescript"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "*clap.js",
      "*clap.ts",
      "coverage",
      "dist",
      "dist-*",
      "docs",
      "gulpfile.js",
      "test",
      "xrun*.js",
      "xrun*.ts"
    ],
    "check-coverage": false,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "cache": false
  },
  "@xarc/module-dev": {
    "features": [
      "eslint",
      "eslintTS",
      "mocha",
      "typedoc",
      "typescript"
    ]
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "@xarc/module-dev/config/test/setup.js"
    ],
    "recursive": true
  }
}
