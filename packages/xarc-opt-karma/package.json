{
  "name": "@xarc/opt-karma",
  "version": "2.0.2",
  "description": "Electrode karma addon for unit tests",
  "main": "xarc-opt-check.js",
  "homepage": "http://www.electrode.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/electrode.git"
  },
