    "karma-spec-reporter": "0.0.34",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.0.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0"
  },
  "devDependencies": {
    "opt-archetype-check": "../opt-archetype-check",
    "shx": "^0.3.4"
  },
  "xarcOptCheck": {
    "optionalTagName": "karma",
