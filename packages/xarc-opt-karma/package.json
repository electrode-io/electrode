  },
  "dependencies": {
    "@types/mocha": "9.1.1",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-shallowly": "^1.0.0",
    "core-js": "^3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "karma": "^6.3.20",
