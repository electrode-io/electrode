{
  "name": "@xarc/jsx-renderer",
  "version": "1.0.13",
  "description": "Render index.htm from JSX template",
  "main": "dist/index.js",
  "scripts": {
    "build": "xrun user/build",
    "test": "xrun xarc/test-only",
    "coverage": "xrun xarc/test-cov",
    "check": "xrun xarc/check",
    "prepublishOnly": "xrun [[build, docs], xarc/check]",
    "docs": "xrun xarc/docs"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "jsx",
    "renderer",
    "index.htm"
  ],
  "author": "Electrode <https://github.com/electrode-io/electrode>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^10.0.1",
    "@types/node": "^13.7.6",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@xarc/module-dev": "^2.2.5",
    "@xarc/run": "^1.1.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-walmart": "^2.2.1",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-jsdoc": "^30.7.9",
    "mocha": "^10.2.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.4",
    "run-verify": "^1.2.5",
    "sinon": "^7.2.6",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.16",
    "stream-to-array": "^2.3.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.17.4",
    "typedoc-plugin-external-module-name": "^3.1.0",
    "typescript": "^3.8.3",
    "xsh": "^0.4.5",
    "xstdout": "^0.1.1"
  },
  "files": [
    "dist",
    "src"
  ],
  "mocha": {
    "require": [
      "@babel/register",
      "ts-node/register",
      "source-map-support/register",
      "@xarc/module-dev/config/test/setup.js"
    ],
    "recursive": true
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
  "dependencies": {
    "@xarc/render-context": "^1.0.10",
    "lodash": "^4.17.15",
    "munchy": "^1.0.9",
    "optional-require": "^1.1.6",
    "require-at": "^1.0.6",
    "xaa": "1.7.1"
  },
  "nyc": {
    "extends": [
      "@istanbuljs/nyc-config-typescript"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "*clap.js",
      "*clap.ts",
      "xrun-tasks.*",
      "babel.config.js",
      "coverage",
      "dist",
      "docs",
      "gulpfile.js",
      "test"
    ],
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "cache": false
  },
  "@xarc/module-dev": {
    "srcDir": [
      "src"
    ],
    "features": [
      "eslint",
      "eslintTS",
      "mocha",
      "typedoc",
      "typescript"
    ]
  }
}
