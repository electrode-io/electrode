{
  "name": "@xarc/webpack-metrics-plugin",
  "version": "1.0.0",
  "description": "Webpack plugin for detailed build performance metrics including cache stats, slowest modules, loader performance, and memory usage",
  "main": "dist-node-cjs/index.js",
  "module": "dist-node-esm/index.js",
  "exports": {
    ".": {
      "import": "./dist-node-esm/index.js",
      "require": "./dist-node-cjs/index.js"
    }
  },
  "homepage": "http://www.electrode.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/electrode.git",
    "directory": "packages/xarc-webpack-metrics-plugin"
  },
  "bugs": {
    "url": "https://github.com/electrode-io/electrode/issues"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "xrun user/build",
    "test": "xrun xarc/test-only",
    "prepublishOnly": "xrun build"
  },
  "keywords": [
    "webpack",
    "plugin",
    "metrics",
    "performance",
    "build",
    "cache",
    "analytics"
  ],
  "author": "Electrode (http://www.electrode.io/)",
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 6"
  },
  "files": [
    "dist-node-cjs",
    "dist-node-esm",
    "src"
  ],
  "devDependencies": {
    "@xarc/run": "~2.3.0",
    "@types/node": "~24.10.1",
    "@xarc/module-dev": "~5.0.0",
    "zx": "~8.8.5"
  }
}
