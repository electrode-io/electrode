{
  "lerna": "2.11.0",
  "version": "independent",
  "command.publish.config": {
    "ignore": ["*.md"]
  },
  "commands": {
    "bootstrap": {
      "ignore": ["ignite-core", "electrode-ignite", "generator-electrode"]
    }
  },
  "fynpo": {
    "publishConfig": {
      "tags": {
        "beta7": {
          "enabled": false,
          "packages": {
            "electrode-archetype-react-app": true,
            "electrode-archetype-react-app-dev": true,
            "electrode-archetype-react-component": true,
            "electrode-archetype-react-component-dev": true,
            "generator-electrode": true,
            "@xarc/create-app": true
          },
          "addToVersion": true
        },
        "xarc8": {
          "enabled": false,
          "packages": {
            "@xarc/app": true,
            "@xarc/app-dev": true,
            "@xarc/create-app": true
          }
        }
      }
    }
  }
}
